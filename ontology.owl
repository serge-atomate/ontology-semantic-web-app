    <owl:NamedIndividual rdf:about="http://www.aifb.uni-karlsruhe.de/Publikationen/viewPublikationOWL/id908instance">
        <rdf:type rdf:resource="http://swrc.ontoware.org/ontology#InProceedings"/>
        <rdf:type rdf:resource="http://swrc.ontoware.org/ontology#Publication"/>
        <swrc:year rdf:datatype="http://www.w3.org/2001/XMLSchema#string">2005</swrc:year>
        <swrc:pages rdf:datatype="http://www.w3.org/2001/XMLSchema#string">278--285</swrc:pages>
        <swrc:title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Intelligent Community Lifecycle Support</swrc:title>
        <swrc:month rdf:datatype="http://www.w3.org/2001/XMLSchema#string">July</swrc:month>
        <swrc:booktitle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Proceedings of the 5th International Conference on Knowledge Management (I-KNOW 05), Graz, Austria, June 29 - July 1, 2005</swrc:booktitle>
        <swrc:author rdf:resource="http://www.aifb.uni-karlsruhe.de/Personen/viewPersonOWL/id2023instance"/>

    <owl:NamedIndividual rdf:about="http://www.aifb.uni-karlsruhe.de/Publikationen/viewPublikationOWL/id125instance">
        <rdf:type rdf:resource="http://swrc.ontoware.org/ontology#InProceedings"/>

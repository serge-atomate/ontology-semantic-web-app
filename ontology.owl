    <!-- http://www.aifb.uni-karlsruhe.de/Publikationen/viewPublikationOWL/id1209instance -->

    <owl:NamedIndividual rdf:about="http://www.aifb.uni-karlsruhe.de/Publikationen/viewPublikationOWL/id1209instance">
        <rdf:type rdf:resource="http://swrc.ontoware.org/ontology#InProceedings"/>
        <rdf:type rdf:resource="http://swrc.ontoware.org/ontology#Publication"/>
        <swrc:year rdf:datatype="http://www.w3.org/2001/XMLSchema#string">2006</swrc:year>
        <swrc:title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Integrating Semantic Web Services for Mobile Access</swrc:title>
        <swrc:month rdf:datatype="http://www.w3.org/2001/XMLSchema#string">May</swrc:month>
        <swrc:booktitle rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Proceedings of the ESWC2006 poster and demo session</swrc:booktitle>
        <swrc:note rdf:datatype="http://www.w3.org/2001/XMLSchema#string">To appear</swrc:note>
        <swrc:abstract rdf:datatype="http://www.w3.org/2001/XMLSchema#string">We present our work in integrating Semantic Web services
for access via mobile devices. We have developed a system,
theWebServiceAccessComponent, that transforms a user request
for a service on a mobile device, to a Web service request
and then selects a matching service from the existing
Web services of the Deutsche Telekom, which provide navigational
and weather information. In this poster, we present
the requirements and design of the WebServiceAccessComponent.</swrc:abstract>
        <swrc:author rdf:resource="http://www.aifb.uni-karlsruhe.de/Personen/viewPersonOWL/id2084instance"/>
        <swrc:author rdf:resource="http://www.aifb.uni-karlsruhe.de/Personen/viewPersonOWL/id2123instance"/>

    <owl:NamedIndividual rdf:about="http://www.aifb.uni-karlsruhe.de/Publikationen/viewPublikationOWL/id560instance">
        <rdf:type rdf:resource="http://swrc.ontoware.org/ontology#Article"/>
        <rdf:type rdf:resource="http://swrc.ontoware.org/ontology#Publication"/>
        <swrc:year rdf:datatype="http://www.w3.org/2001/XMLSchema#string">1998</swrc:year>
        <swrc:journal rdf:datatype="http://www.w3.org/2001/XMLSchema#string">LNCS, Springer</swrc:journal>
        <swrc:title rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Ontobroker in a Nutshell</swrc:title>

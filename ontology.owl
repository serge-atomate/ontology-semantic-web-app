    </owl:NamedIndividual>
    


    <!-- http://www.aifb.uni-karlsruhe.de/Publikationen/viewPublikationOWL/id460instance -->

    <owl:NamedIndividual rdf:about="http://www.aifb.uni-karlsruhe.de/Publikationen/viewPublikationOWL/id460instance">

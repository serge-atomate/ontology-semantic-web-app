    <!-- http://www.aifb.uni-karlsruhe.de/Publikationen/viewPublikationOWL/id1266instance -->

    <owl:NamedIndividual rdf:about="http://www.aifb.uni-karlsruhe.de/Publikationen/viewPublikationOWL/id1266instance">
        <rdf:type rdf:resource="http://swrc.ontoware.org/ontology#InProceedings"/>

        <swrc:author rdf:resource="http://www.aifb.uni-karlsruhe.de/Publikationen/viewExternerAutorOWL/id622instance"/>
        <swrc:author rdf:resource="http://www.aifb.uni-karlsruhe.de/Publikationen/viewExternerAutorOWL/id7instance"/>
        <swrc:author rdf:resource="http://www.aifb.uni-karlsruhe.de/Publikationen/viewExternerAutorOWL/id84instance"/>
    </owl:NamedIndividual>
    


